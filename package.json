{
	"name": "vscode-backup",
	"displayName": "VScode Backup",
	"publisher": "Westenets",
	"repository": {
		"type": "git",
		"url": "https://github.com/DK013/vscdoe-backup.git"
	},
	"homepage": "https://github.com/DK013/vscdoe-backup",
	"description": "Backup/Restore VSCode Settings and plugins",
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#101010",
		"theme": "dark"
	},
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.backup",
		"onCommand:extension.restore",
		"*"
	],
	"keywords": [
		"Backup",
		"Extainsion Backup",
		"Plugins Backup",
		"Settings Backup"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.backup",
				"title": "VSCode Backup"
			},
			{
				"command": "extension.restore",
				"title": "VSCode Restore"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js",
		"build": "vsce package"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vsce": "^1.73.0",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"adm-zip": "^0.4.13",
		"node-powershell": "^4.0.0",
		"opn": "^6.0.0",
		"shelljs": "^0.8.3"
	}
}
